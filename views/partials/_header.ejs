<header id="pagetop">
    <a class="nav-logo" href="/">dead.Stock</a>
    <a class="nav-link" href="/create">Create Listing</a>
    <% if (!currentUser) { %>
      <a class="nav-user" href="/session/login">Login</a>
      <a class="nav-user" href="/session/register">Register</a>
      <% } else { %>
        <a class="nav-link" href="/users/<%= currentUser %>/listings">My Listings</a>
        <form method="POST" action="/session/logout" class="nav-user">Logged in as: <%= userDetails.email %>
            <button type="submit" class="nav-user">Logout</button></form>
        <% } %>
</header>
