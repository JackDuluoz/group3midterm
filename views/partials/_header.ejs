<header>
    <a class="nav-logo" href="/"><code>dead.Stock</code></a>
    <a class="nav-link" href="/create">Create Listing</a>
    <% if (!currentUser) { %>
    <a class="nav-user" href="/session/login">Login</a>
    <a class="nav-user" href="/session/register">Register</a>
    <% } else { %>
    <a class="nav-link" href="/users/<%= currentUser %>/listings">My Listings</a>
    <form method="POST" action="/session/logout" class="nav-email">Logged in as: <%= userDetails.email %>
    <button type="submit" class="logout">Logout</button></form>
    <% } %>
</header>
